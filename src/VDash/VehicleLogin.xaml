    <Window x:Class="VDash.VehicleLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="VehicleLogin" Height="150" Width="300">
    <Grid Height="110">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="299*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Label Content="Password" HorizontalAlignment="Left" Margin="16,25,0,0" VerticalAlignment="Top" Width="70" />
        <TextBox HorizontalAlignment="Left" Margin="93,28,0,0" Name="TextBoxPassword" VerticalAlignment="Top" Width="170" />
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="10,50,0,0" Name="BtnLogin" VerticalAlignment="Top" Width="75" Click="btnLogin_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="190,50,0,0" Name="BtnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
